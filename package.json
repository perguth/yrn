  "bugs": {
    "url": "https://github.com/pguth/yrn/issues"
  },
  "homepage": "https://github.com/pguth/yrn#readme",
  "dependencies": {
    "find-root": "^1.0.0",
    "jsonfile": "^2.4.0",
    "run-parallel": "^1.1.6",
    "yargs": "^7.0.1"
  },
  "peerDependencies": {
    "yarn": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
